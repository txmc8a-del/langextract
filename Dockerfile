function closeHelp(){
	   $("#helpPopup").attr("src","/viewer/blank.html");
	   $("#helpPopup").hide();
	   $("#overlay").hide();
}

//adds two brs to put already registered button on next line on mobile
function addLineBreakOnMobile(){
	if($(window).width() < 415){
		$('.break').remove();
		$("<br class='break'/><br class='break'/>").insertAfter('#footerRegisterBtn');
		$("<br class='break'/><br class='break'/>").insertAfter('#headerRegisterBtn');				
	}else{
		$('.break').remove();
	}	
}

